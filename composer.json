{
	"name" : "keeko/user",
	"description" : "Keeko user module",
	"type" : "keeko-module",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.4"
	},
	"require-dev" : {
		"composer/composer" : "@dev",
		"keeko/core" : "dev-master",
		"propel/propel" : "@dev"
	},
	"autoload" : {
		"psr-4" : {
			"keeko\\user\\" : "src"
		}
	},
	"extra" : {
		"keeko" : {
			"module" : {
				"title" : "Keeko User Module",
				"default-action" : "users",
				"class" : "\\keeko\\user\\UserModule",
				"actions" : {
					"users" : {
						"class" : "\\keeko\\user\\action\\ListAction",
						"title" : "List Users",
						"description" : "Lists all users",
						"response" : {
							"json" : "\\keeko\\user\\response\\ListJsonResponse"
						}
					},
					"user" : {
						"class" : "\\keeko\\user\\action\\ReadAction",
						"title" : "Reads a user",
						"params" : {
							"id" : {
								"title" : "User identificator",
								"required" : true
							}
						}
					},
					"user-create" : {
						"class" : "\\keeko\\user\\action\\CreateAction",
						"title" : "Creates a user",
						"params" : {
							"mail" : {
								"title" : "The user's email adress"
							},
							"given-name" : {
								"title" : "The user's given name"
							},
							"family-name" : {
								"title" : "The user's family name"
							}
						}
					},
					"user-update" : {
						"class" : "\\keeko\\user\\action\\UpdateAction",
						"title" : "Updates a user",
						"params" : {
							"id" : {
								"title" : "User identificator",
								"required" : true
							},
							"mail" : {
								"title" : "The user's email adress"
							},
							"given-name" : {
								"title" : "The user's given name"
							},
							"family-name" : {
								"title" : "The user's family name"
							}
						}
					},
					"user-delete" : {
						"class" : "\\keeko\\user\\action\\DeleteAction",
						"title" : "Deletes a user",
						"params" : {
							"id" : {
								"title" : "User identificator",
								"required" : true
							}
						}
					}
				},
				"api" : {
				    "swaggerVersion": "1.2",
					"resourcePath" : "/user",
					"apis" : [{
							"path" : "/users",
							"operations" : [{
									"method" : "GET",
									"summary" : "Retrieves all users",
									"nickname" : "users",
									"type" : "PagedUsers",
									"responseMessages" : [{
											"code" : 403,
											"message" : "Permission Denied"
										}
									]
								}, {
									"method" : "POST",
									"summary" : "Create a User",
									"nickname" : "user-create",
									"type" : "User",
									"parameters" : [{
											"name" : "body",
											"description" : "The new user object",
											"required" : true,
											"type" : "User",
											"paramType" : "body"
										}
									],
									"responseMessages" : [{
											"code" : 403,
											"message" : "Permission Denied"
										}, {
											"code" : 500,
											"message" : "Something was wrong"
										}
									]
								}
							]
						}, {
							"path" : "/users/{id}",
							"operations" : [{
									"method" : "GET",
									"summary" : "Reads a User",
									"nickname" : "user",
									"type" : "User",
									"parameters" : [{
											"name" : "id",
											"description" : "The user id",
											"required" : true,
											"type": "id",
											"paramType" : "path"
										}
									],
									"responseMessages" : [{
											"code" : 403,
											"message" : "Permission Denied"
										}, {
											"code" : 500,
											"message" : "Something was wrong"
										}, {
											"code" : 400,
											"message" : "Invalid ID supplied"
										}, {
											"code" : 404,
											"message" : "No user found"
										}
									]
								}, {
									"method" : "PATCH",
									"summary" : "Updates a user",
									"nickname" : "user-update",
									"type" : "User",
									"parameters" : [{
											"name" : "id",
											"description" : "The user id",
											"required" : true,
											"type": "id",
											"paramType" : "path"
										}
									],
									"responseMessages" : [{
											"code" : 403,
											"message" : "Permission Denied"
										}, {
											"code" : 500,
											"message" : "Something was wrong"
										}, {
											"code" : 400,
											"message" : "Invalid ID supplied"
										}, {
											"code" : 404,
											"message" : "No user found"
										}
									]
								}, {
									"method" : "DELETE",
									"action" : "user-delete",
									"summary" : "Deletes a user",
									"nickname" : "user-delete",
									"type" : "string",
									"parameters" : [{
											"name" : "id",
											"description" : "The user id",
											"required" : true,
											"type": "id",
											"paramType" : "path"
										}
									],
									"responseMessages" : [{
											"code" : 403,
											"message" : "Permission Denied"
										}, {
											"code" : 500,
											"message" : "Something was wrong"
										}, {
											"code" : 400,
											"message" : "Invalid ID supplied"
										}, {
											"code" : 404,
											"message" : "No user found"
										}
									]
								}
							]
						}
					],
					"models" : {
						"Meta" : {
							"id" : "Meta",
							"properties" : {
								"total" : {
									"type" : "integer"
								},
								"next" : {
									"type" : "string"
								},
								"previous" : {
									"type" : "string"
								},
								"first" : {
									"type" : "string"
								},
								"last" : {
									"type" : "string"
								}
							}
						},
						"PagedUsers" : {
							"id" : "PagedUsers",
							"properties" : {
								"users" : {
									"type" : "array",
									"items" : {
										"$ref" : "User"
									}
								},
								"meta" : {
									"type" : "Meta"
								}
							}
						},
						"User" : {
							"id" : "User",
							"properties" : {
								"id" : {
									"type" : "integer",
									"required" : true
								},
								"mail" : {
									"type" : "string",
									"required" : true
								}
							}
						}
					}
				}
			}
		},
		"local" : "../../"
	}
}