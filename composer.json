{
	"name" : "keeko/user",
	"description" : "Keeko user module",
	"type" : "keeko-module",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.4"
	},
	"require-dev" : {
		"composer/composer" : "@dev",
		"keeko/core" : "dev-master",
		"propel/propel" : "@dev"
	},
	"autoload" : {
		"psr-4" : {
			"keeko\\user\\" : "src"
		}
	},
	"extra" : {
		"keeko" : {
			"module" : {
				"title" : "Keeko User Module",
				"default-action" : "users",
				"class" : "\\keeko\\user\\UserModule",
				"actions" : {
					"users" : {
						"class" : "\\keeko\\user\\action\\UserListAction",
						"title" : "List Users",
						"description" : "Lists all users"
					},
					"user" : {
						"class-prefix" : "\\keeko\\user\\action\\ReadAction",
						"title" : "Reads a user",
						"params" : {
							"id" : {
								"id" : true,
								"title" : "User identificator",
								"required" : true
							}
						}
					},
					"user-create" : {
						"class-prefix" : "\\keeko\\user\\action\\CreateAction",
						"title" : "Creates a user",
						"params" : {
							"mail" : {
								"title" : "The user's email adress"
							},
							"given-name" : {
								"title" : "The user's given name"
							},
							"family-name" : {
								"title" : "The user's family name"
							}
						}
					},
					"user-update" : {
						"class-prefix" : "\\keeko\\user\\action\\UpdateAction",
						"title" : "Updates a user",
						"params" : {
							"id" : {
								"id" : true,
								"title" : "User identificator",
								"required" : true
							},
							"mail" : {
								"title" : "The user's email adress"
							},
							"given-name" : {
								"title" : "The user's given name"
							},
							"family-name" : {
								"title" : "The user's family name"
							}
						}
					},
					"user-delete" : {
						"class" : "\\keeko\\user\\action\\DeleteAction",
						"title" : "Deletes a user",
						"params" : {
							"id" : {
								"id" : true,
								"title" : "User identificator",
								"required" : true
							}
						}
					}
				},
				"api" : {
					"routes" : {
						"users" : [{
								"method" : "get",
								"action" : "users"
							}, {
								"method" : "post",
								"action" : "user-create"
							}
						],
						"users/:id" : [{
								"method" : "get",
								"action" : "user-read"
							}, {
								"method" : "patch",
								"action" : "user-update"
							}, {
								"method" : "delete",
								"action" : "user-delete"
							}
						]
					}
				}
			}
		},
		"local" : "../../"
	}
}